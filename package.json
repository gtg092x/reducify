{
  "name": "reducify",
  "version": "1.0.0",
  "description": "Make reducers with less effort.",
  "main": "lib/reducify.js",
  "scripts": {
    "test": "./node_modules/.bin/cross-env BABEL_ENV=commonjs ./node_modules/.bin/mocha --compilers js:babel-register --recursive",
    "compile": "./node_modules/.bin/babel --source-maps inline --optional runtime -d lib/ src/",
    "prepublish": "npm run compile",
    "ghpage": "lessc ghpage/style.less ghpage.css && pug ./ghpage/index.pug --out ./"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "stage-2"
    ]
  },
  "nyc": {
    "exclude": [
      "*.js",
      "lib/*.js",
      "coverage/*.js",
      "**/__tests__/*.js",
      "**/__tests__/fixtures/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gtg092x/reducify.git"
  },
  "keywords": [
    "redux",
    "reducify",
    "reducers"
  ],
  "author": "Matthew Drake <mdrake@mediadrake.com> (https://github.com/gtg092x)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gtg092x/reducify/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "coveralls": "^2.11.8",
    "cross-env": "^2.0.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-prometheusresearch": "^0.2.0",
    "eslint-plugin-import": "^1.8.1",
    "jsdom": "^6.3.0",
    "jstransformer-marked": "^1.0.1",
    "mocha": "^2.5.3",
    "node-libs-browser": "^1.0.0",
    "power-assert": "^1.3.1",
    "redux": "^3.5.2"
  },
  "dependencies": {
    "lodash": "^4.14.1"
  }
}
